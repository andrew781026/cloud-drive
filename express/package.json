{
  "name": "cloud-drive-express",
  "version": "0.0.1",
  "description": "the cloud drive express server",
  "scripts": {
    "start": "nodemon src/app.js",
    "dev:start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/app.ts",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/app.ts"
  },
  "author": "andrew",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.10",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.0"
  }
}
